{
    "name": "bolt-oss/article",
    "description": "📝 Bolt Extension to add the Article FieldType",
    "type": "bolt-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Bob den Otter",
            "email": "bob@twokings.nl"
        }
    ],
    "require": {
        "php": ">=7.2.9"
    },
    "require-dev": {
        "bolt-oss/core": "^5.0",
        "symplify/easy-coding-standard": "^7.0"
    },
    "replace": {
        "bolt/article": "*"
    },
    "autoload": {
        "psr-4": {
            "Bolt\\Article\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "entrypoint": "Bolt\\Article\\Extension",
        "screenshots": [
            "screenshots/article.png"
        ]
    }
}
